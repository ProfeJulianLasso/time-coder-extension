{
  "name": "sofka-time-coder-reports",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "clean:src": "node clean-js-files.js",
    "prebuild": "npm run clean && npm run clean:src",
    "build": "NODE_ENV=production webpack --mode production --config webpack.config.js && npm run clean:src",
    "predev": "npm run clean && npm run clean:src",
    "dev": "NODE_ENV=production webpack --mode development --watch --config webpack.config.js",
    "prestart": "npm run clean",
    "start": "NODE_ENV=development webpack serve --config webpack.dev.js"
  },
  "dependencies": {
    "@preact/signals-react": "^3.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/vscode": "^1.98.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.11.0",
    "glob": "^8.1.0",
    "html-webpack-plugin": "^5.6.3",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.2",
    "webpack": "^5.98.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.2"
  }
}